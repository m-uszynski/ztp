        <Label x:Name="ProductNameLabel" Content="Nazwa:" HorizontalAlignment="Left" Margin="13,15,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ProductCountLabel" Content="Ilość w magazynie:" HorizontalAlignment="Left" Margin="13,46,0,0" VerticalAlignment="Top"/>
        <Label x:Name="Count" Content="Ile:" HorizontalAlignment="Left" Margin="13,104,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="countTextBox" HorizontalAlignment="Left" Height="23" Margin="44,104,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="backBtn" Content="Anuluj" HorizontalAlignment="Left" Margin="109,174,0,0" VerticalAlignment="Top" Width="75" Click="backBtn_Click"/>
        <Button x:Name="addBtn" Content="Dodaj" HorizontalAlignment="Left" Margin="13,174,0,0" VerticalAlignment="Top" Width="75" Click="addBtn_Click"/>

    </Grid>
</Window>
